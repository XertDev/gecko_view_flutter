{
  "manifest_version": 2,
  "name": "host-js-execution",
  "version": "1.0",
  "description": "Host js executor",
  "browser_specific_settings": {
    "gecko": {
      "id": "host-js-execution@xert.info"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "nativeMessagingFromContent",
    "geckoViewAddons",
    "tabs",
    "<all_urls>"
  ]
}